{
  "name": "@vortex/config",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check . --ignore-path ../../.prettierignore",
    "format:fix": "prettier --write . --ignore-path ../../.prettierignore",
    "clean": "rimraf dist node_modules",
    "clean:turbo": "rimraf .turbo dist node_modules"
  },
  "dependencies": {
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/node": "^25.2.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  }
}
